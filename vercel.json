{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    },
    "pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  },
  "regions": ["iad1"]
}
